let Point2D = struct(point)
  type t = { x : Int.t ; y : Int.t }
  let init = fun(x : Int.t, y : Int.t) -> { x = x ; y = y }
  let first = fun(p : point.t) -> p.x
  let second = fun(p : point.t) -> p.y
  let dummy = fun(p1 : { x : Int.t ; y : Int.t }, p2 : point.t) ->
    let first = point.first in
    let second = point.second in
    { x = first p1 ; y = second p2 }
end;;
